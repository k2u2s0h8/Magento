<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
     <body>		
     	<referenceBlock name="footer_links">
  			<block class="Magento\Framework\View\Element\Html\Link\Current" name="custom-link">
    			<arguments>
      				<argument name="label" xsi:type="string">Student Detail</argument>
      				<argument name="path" xsi:type="string">form/fatchdetail/detail</argument>
    			</arguments>
  			</block>
		</referenceBlock>	  
        <referenceContainer name="content">			
			<block class="Detail\Form\Block\FatchDetail\Detail" name="fatchdetail_detail" template="fatchdetail/detail.phtml">
			</block>
        </referenceContainer>      
    </body>
</page>
